&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| NULL
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| cipher preference: server
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| TLSv1.2: 
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| ciphers: 
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256 &lt;span style="color:#f92672">(&lt;/span>prime256v1&lt;span style="color:#f92672">)&lt;/span> - A
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256 &lt;span style="color:#f92672">(&lt;/span>prime256v1&lt;span style="color:#f92672">)&lt;/span> - A
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256-draft &lt;span style="color:#f92672">(&lt;/span>prime256v1&lt;span style="color:#f92672">)&lt;/span> - A
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| TLS_ECDHE_ECDSA_WITH_AES_128_CBC_SHA &lt;span style="color:#f92672">(&lt;/span>prime256v1&lt;span style="color:#f92672">)&lt;/span> - A
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| TLS_ECDHE_ECDSA_WITH_AES_128_CBC_SHA256 &lt;span style="color:#f92672">(&lt;/span>prime256v1&lt;span style="color:#f92672">)&lt;/span> - A
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>| TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384 &lt;span style="color:#f92672">(&lt;/span>prime256v1&lt;span style="color:#f92672">)&lt;/span> - A
